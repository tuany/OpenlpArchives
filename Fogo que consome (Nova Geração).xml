<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.2" modifiedIn="OpenLP 2.2" modifiedDate="2016-01-29T21:44:12">
  <properties>
    <titles>
      <title>Fogo que consome</title>
    </titles>
    <authors>
      <author>Nova Geração</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Fogo que consome nós te desejamos</lines>
    </verse>
    <verse name="v2">
      <lines>Chamas do Amor , Batiza-nos, batiza-nos</lines>
    </verse>
    <verse name="v3">
      <lines>Nos dê mais paixão por Ti</lines>
    </verse>
    <verse name="v4">
      <lines>Quão profundo do Alto do Lago é Teu Amor</lines>
    </verse>
  </lyrics>
</song>
