<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.2" modifiedIn="OpenLP 2.2" modifiedDate="2016-01-26T07:39:31">
  <properties>
    <titles>
      <title>Quem ja pisou</title>
      <title>Quem ja pisou</title>
    </titles>
    <authors>
      <author>Voz de muitas águas</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Eu me rasgo, por inteiro faço tudo<br/>mas vem novamente</lines>
    </verse>
    <verse name="v2">
      <lines>Eu mergulho na mirra ardente</lines>
    </verse>
    <verse name="v3">
      <lines>mas peço que tua presença aumente</lines>
    </verse>
    <verse name="v5">
      <lines>longe dos santo dos santos,<br/>não sei mais viver</lines>
    </verse>
  </lyrics>
</song>
