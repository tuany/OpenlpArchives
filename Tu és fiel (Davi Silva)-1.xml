<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.2" modifiedIn="OpenLP 2.2" modifiedDate="2015-12-27T20:19:04">
  <properties>
    <titles>
      <title>Tu és fiel</title>
    </titles>
    <authors>
      <author>Davi Silva</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Como não lembrar<br/>Teu amor por mim</lines>
    </verse>
    <verse name="v2">
      <lines>Quero expressar<br/>Como sou grato Senhor</lines>
    </verse>
    <verse name="v3">
      <lines>Como não cantar <br/>E te adorar</lines>
    </verse>
    <verse name="v4">
      <lines>Quero expressar<br/>Como sou grato Senhor</lines>
    </verse>
    <verse name="v5">
      <lines>Tu és fiel</lines>
    </verse>
    <verse name="v6">
      <lines>Aleluia</lines>
    </verse>
    <verse name="v7">
      <lines>Tua fidelidade é grande</lines>
    </verse>
  </lyrics>
</song>
